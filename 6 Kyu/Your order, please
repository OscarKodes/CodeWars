function order(words){
  
  let resultArr = [];
  
  let arr = words.split(' ');
  
  for (let i = 1; i < 10; i++) {
  
    for (let j = 0; j < arr.length; j++) {
    
      let word = arr[j];
      
      if (word.includes(i)) {
      
        resultArr.push(word);
        
        break;
      }
    }
  }
  
  return resultArr.join(' ');
}
