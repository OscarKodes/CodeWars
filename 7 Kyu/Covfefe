function covfefe(str){

  if (str.includes("coverage")) {
  
    for (let i = 0; i < str.length - 7; i++) {
    
      let mySlice = str.slice(i, i + 8);
      
      if (mySlice === "coverage") {
      
        str = str.slice(0, i) + "covfefe" + str.slice(i + 8);
      }
    }
  } else {
  
    str += " covfefe";
  }
  
  return str;
}
