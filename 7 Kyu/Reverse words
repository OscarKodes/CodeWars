function reverseWords(str) {

  let strArr = str.split(" ");
  let arr = [];
  
  for (let j = 0; j < strArr.length; j++) {
  
    let word = strArr[j];
    let reverseWord = "";
  
    for (let i = word.length - 1; i >= 0; i--) {
  
      reverseWord += word[i];
    }
    
    arr.push(reverseWord);
  }
  
  return arr.join(" ");
}
