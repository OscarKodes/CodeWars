class SmallestIntegerFinder {

    findSmallestInt(args) {
      
      let smallestInt = args[0];
      
      for (let i = 0; i < args.length; i++) {
      
        let currNum = args[i];
        
        if (currNum < smallestInt) {
        
          smallestInt = currNum;
        }
      }
      
    return smallestInt;
  }
}
