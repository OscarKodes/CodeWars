function zero(arr) {
  
  if (arr) {
    
    if (arr[1] === '+') {
    
      return 0 + arr[0];
    }
    
    if (arr[1] === '-') {
    
      return 0 - arr[0];
    }
    
    if (arr[1] === '*') {
    
      return 0 * arr[0];
    }
    
    if (arr[1] === '/') {
    
      return (0 - (0 % arr[0])) / arr[0];
    }
  }
  
  else {
    return 0;
  }
}

function one(arr) {
  
  if (arr) {
    
    if (arr[1] === '+') {
    
      return 1 + arr[0];
    }
    
    if (arr[1] === '-') {
    
      return 1 - arr[0];
    }
    
    if (arr[1] === '*') {
    
      return 1 * arr[0];
    }
    
    if (arr[1] === '/') {
    
      return (1 - (1 % arr[0])) / arr[0];
    }
  }
  
  else {
    return 1;
  }
}

function two(arr) {
  
  if (arr) {
    
