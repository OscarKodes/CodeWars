    }
  }
  
  else {
    return 9;
  }}
  
  

function plus(num1) {
  
  let arr = [num1, '+'];
  
  return arr;
}

function minus(num1) {
  
  let arr = [num1, '-'];
  
  return arr;
}

function times(num1) {
  
  let arr = [num1, '*'];
  
  return arr;
}

function dividedBy(num1) {
  
  let arr = [num1, '/'];
  
  return arr;

}
